{
  "name": "wasmer-runtime",
  "type": "module",
  "dependencies": {
    "unbuild": "^2.0.0",
    "start-server-and-test": "^2.0.3"
  },
  "scripts": {
    "build": "unbuild",
    "start-server-wasmer": "wasmer run wasmer/winterjs --net --env PORT=7070 --mapdir dist:dist dist/handler.mjs",
    "start-server": "winterjs dist/handler.mjs",
    "run": "curl -o data.json http://127.0.0.1:7070",
    "start-wasmer": "start-server-and-test start-server-wasmer http://127.0.0.1:7070 run",
    "start": "start-server-and-test start-server http://127.0.0.1:7070 run"
  },
  "unbuild": {
    "entries": [
      "handler.ts"
    ],
    "clean": true,
    "rollup": {
      "esbuild": {
        "platform": "neutral",
        "target": "esnext"
      }
    }
  }
}